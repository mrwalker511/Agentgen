{
  "name": "agentgen",
  "version": "0.1.0",
  "description": "AI-first project scaffolding with AGENT.md as a first-class artifact",
  "keywords": [
    "scaffolding",
    "generator",
    "cli",
    "agent",
    "ai",
    "fastapi",
    "express",
    "typescript"
  ],
  "homepage": "https://github.com/mrwalker511/Agentgen#readme",
  "bugs": {
    "url": "https://github.com/mrwalker511/Agentgen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrwalker511/Agentgen.git"
  },
  "license": "MIT",
  "author": "Agentgen Team",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agentgen": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "packs",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "cli": "node --loader ts-node/esm src/cli/index.ts"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "handlebars": "^4.7.8",
    "zod": "^3.22.4",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.6",
    "@types/handlebars": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
